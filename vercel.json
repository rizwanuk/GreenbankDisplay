{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },

    { "src": "/api/met/(.*)", "dest": "/api/met.js?path=$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },

    { "src": "/embed", "dest": "/embed.html" },

    { "src": "/slideshow", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
